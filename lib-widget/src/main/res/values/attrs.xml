<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CustomClearText">
        <attr name="ctIcon" format="reference" />
        <attr name="ctHint" format="string|reference" />
        <attr name="ctText" format="string|reference" />
        <attr name="ctTextSize" format="dimension" />
        <attr name="ctTextColor" format="color|reference" />
        <attr name="ctTextStyle" format="enum">
            <enum name="normal" value="0" />
            <enum name="bold" value="1" />
            <enum name="italic" value="2" />
        </attr>
    </declare-styleable>
    <declare-styleable name="xSeekBar">
        <attr name="interval" format="integer" />
    </declare-styleable>

    <!-- 垂直方向的虚线 -->
    <declare-styleable name="DividerView">
        <!-- 虚线颜色 -->
        <attr name="divider_line_color" format="color" />
        <!-- 虚线宽度 -->
        <attr name="dashThickness" format="dimension" />
        <!-- 虚线dash长度 -->
        <attr name="dashLength" format="dimension" />
        <!-- 虚线dash间隔 -->
        <attr name="dashGap" format="dimension" />
        <!-- 虚线朝向 -->
        <attr name="divider_orientation" format="enum">
            <enum name="horizontal" value="0" />
            <enum name="vertical" value="1" />
        </attr>
    </declare-styleable>
</resources>
